set terminal postscript eps color;
set out '../../LaTeX/task5.eps';
#set title "integrand lookback option, M=2, S(0)=10, r=0.05, sigma=0.2, T=1, K=10,";
s(x)=10*exp((0.05-0.5*0.2**2)*1+0.2*1*x);
f(x)=(s(x)-10);
set xrange [0:10];
set yrange [0:10];
set xlabel "x";
set ylabel "y";
set isosample 40;
set view 70,340;
splot (s(x)>s(y) ? (f(x)>0?f(x):0) : (f(y)>0?f(y):0)) notitle;

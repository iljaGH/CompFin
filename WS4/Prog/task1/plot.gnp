set terminal postscript eps color;
set out '../../LaTeX/task1.eps';
#set title "integrand two-dimensional down-out call, M=2, S(0)=10, r=0.05, sigma=0.2, T=1, K=10, B=8";
s(x)=10*exp((0.05-0.5*0.2**2)*1+0.2*1*x);
f(x)=(s(x)-10);
set xrange [0:10];
set yrange [0:10];
set xlabel "x";
set ylabel "y";
set isosample 40;
set view 60,330;
splot (s(x)>8 ? (s(y)>8 ? (f(x)>0 ? f(x) : 0) : 0) : 0) notitle;
